# MLB Database Configuration
# Copy this file to .env and update with your actual database credentials

# Database host (usually localhost for local development)
MLB_DB_HOST=localhost

# Database port (default MySQL port is 3306)
MLB_DB_PORT=3306

# Database name
MLB_DB_NAME=mlb

# Database user
MLB_DB_USER=your_user

# Database password
MLB_DB_PASS=your_pass
